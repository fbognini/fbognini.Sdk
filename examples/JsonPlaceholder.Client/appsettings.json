{
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [],
    "Enrich": [
      "FromLogContext"
    ],
    "Properties": {
      "Application": "JsonPlaceholder.Client"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Hangfire": "Warning",
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Error",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs/log.log",
          "outputTemplate": "{Timestamp} [{Level:u3}] [{RequestId}] {Message:lj}{NewLine}{Exception}",
          "rollingInterval": "Day",
          "restrictedToMinimumLevel": "Information",
          "retainedFileCountLimit": 5
        }
      },
      {
        "Name": "Seq"
        // ENV
      }
    ]
  }
}
